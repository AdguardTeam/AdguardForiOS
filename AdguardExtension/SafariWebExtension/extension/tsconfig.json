{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "module": "commonjs",
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "jsx": "react",
    "resolveJsonModule": true,
  }
}
